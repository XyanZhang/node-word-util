          <w:kern w:val="0"/>
          <w:sz w:val="20"/>
        </w:rPr>
        <w:t>或</w:t>
      </w:r>
      <w:r w:rsidRPr="00766599">
        <w:rPr>
          <w:rFonts w:ascii="微软雅黑" w:eastAsia="微软雅黑" w:hAnsi="微软雅黑" w:cs="宋体" w:hint="eastAsia"/>
